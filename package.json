		"postcss": "^8.4.35",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"prettier-plugin-tailwindcss": "^0.5.11",
		"svelte": "4.2.12",
		"svelte-check": "^3.6.4",
		"sveltekit-superforms": "^2.8.1",
		"tailwindcss": "^3.4.1",

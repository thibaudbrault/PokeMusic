		"lint:fix": "eslint --fix",
		"studio": "drizzle-kit studio --config drizzle.config.ts",
		"generate": "drizzle-kit generate:pg --config drizzle.config.ts",
		"migrate": "drizzle-kit push:pg --config drizzle.config.ts",
		"prepare": "husky"
	},
	"devDependencies": {
		"@sveltejs/adapter-node": "^5.0.1",
		"@sveltejs/kit": "^2.0.0",
